{
  "name": "hapi-react-redux-hmr",
  "version": "0.0.2",
  "description": "A boilerplate project with Hapi React Redux and Webpack with HMR in development mode",
  "main": "./bin/server",
  "scripts": {
    "dev": "nodemon ./bin/serverdev  --hot",
    "test": "lab --transform ./test/setup.js --ignore 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__'",
    "compile-ui": "NODE_ENV=production rm -rf web/site/; webpack --colors --display-error-details --config configs/webpack.client.js",
    "compile-server": "rm -rf dist/; babel -d dist/ lib/",
    "compile": "npm run compile-server; npm run compile-ui",
    "preuninstall": "rm -rf dist/; rm -rf web/site/",
    "install": "npm run compile"
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.6.x",
    "babel-core": "^6.7.x",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.x",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.x",
    "babel-register": "^6.7.x",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "h2o2": "^5.1.0",
    "hapi": "^13.2.2",
    "hapi-swagger": "^4.3.1",
    "html-webpack-plugin": "^2.15.0",
    "inert": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "radium": "^0.17.0",
    "react": "^0.14.8",
    "react-bootstrap": "^0.28.4",
    "react-dom": "^0.14.8",
    "react-intl": "^2.0.0-rc-1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vision": "^4.0.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "code": "^2.1.0",
    "lab": "^10.3.0",
    "nodemon": "^1.8.1",
    "react-transform-hmr": "^1.0.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "author": "Jeremy Darling <jeremy.darling@gmail.com>",
  "license": "MIT"
}
